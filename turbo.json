{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["BASE_URL", "SITE"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["node_modules", "dist"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["node_modules", "dist"]
    },
    "clean": {}
  }
}
